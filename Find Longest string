#include <iostream>
using namespace std;
int main()
{
    // Number of rows
    int N,maxi=0;
    cout<<"Enter the value:";
    cin>>N;
    string s[100],res="";
    cout<<"Enter the Strings "<<endl;
    for (int i = 0; i < N; i++) 
    {
    cin>>s[i];
    if(s[i].size()>maxi)
    {
        maxi=s[i].size();
        res=s[i];
    }
    }
    cout<<"Longest string is "<<res;
    
    return 0;
}
